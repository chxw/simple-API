<!DOCTYPE html>
<html >

<head>
    <meta charset="utf-8">
</head>

<body>
    <section class="home">
    <h1>Passengers</h1>
    <table>
        <% for(var i=0; i<passenger.length; i++) {%>
            <tr>
                <td> <%= passenger[i].username %> </td>
                <br/>
                <td> <%= passenger[i].lat %> </td>
                <br/>
                <td> <%= passenger[i].lng %> </td>
            </tr>
        <% } %>
    </table>
    </section>
</body>

</html>
