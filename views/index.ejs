<!DOCTYPE html>
<html >

<head>
    <meta charset="utf-8">
</head>

<body>
    <section class="home">
    <h1>Passengers</h1>

    <li><%= passenger %></li>

    <ul>
        <% for(var i=0; i<passenger.length; i++) {%>
            <li>
                <span>Username: </span><%= passenger[i].username %>
                <br/>
                <span>Lat: </span><%= passenger[i].lat %>
                <br/>
                <span>Lng: </span><%= passenger[i].lng %>
            </li>
        <% } %>
    </ul>
    </section>
</body>

</html>
